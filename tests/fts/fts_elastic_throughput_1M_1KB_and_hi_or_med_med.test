[test_case]
test = perfrunner.tests.elastic.ElasticThroughputTest
title = AndMedOrHighHigh, no kv-load, wiki 1M x 1KB
summary = Elasticsearch throughput Test
larger_is_better = false


[access]
time = 600

[cluster]
mem_quota = 51024
initial_nodes = 1
num_buckets = 1
fts_index_mem_quota = 20000


[fts]
items = 1000000
name  = perf_elastic_index
port = 8094
worker= 10
type = 1_conjuncts_2_disjuncts
query_file = perfrunner/tests/FTS/mid_hi_hi.txt
query_size = 10
logfile = /tmp/fts_throughput_AndMedOrHighHigh_1M_1K.txt
orderby = q0b00001000
backup_path = /home/backup
repo_path = wiki1M
field = text
elastic = 1