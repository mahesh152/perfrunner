[test_case]
test = perfrunner.tests.elastic.ElasticThroughputTest
title = High Term, no kv-load, wiki 1M x 1KB
summary = ELasticSearch throughput test
larger_is_better = true

[fts]
items = 1000000
name  = perf_elastic_index
port = 9091
worker= 10
type = term
query_file = /tests/fts/data/hiterm.txt
query_size = 10
elastic = 1
logfile = /tmp/elastic_thput_termhi_1M_1K.txt
orderby = q0b00000001
backup_path = /home/backup
repo_path = wiki1M
field = text

[access]
time = 600

[cluster]
mem_quota = 51024
initial_nodes = 1
num_buckets = 1
