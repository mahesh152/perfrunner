[test_case]
test = perfrunner.tests.elastic.ElasticLatencyTest
title = OrHighMed, no kv-load, wiki 1M x 1KB
summary = Elasticsearch latency Test
larger_is_better = false

[access]
time = 1200

[cluster]
mem_quota = 51024
initial_nodes = 1
